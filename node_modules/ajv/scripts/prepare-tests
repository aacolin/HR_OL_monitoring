#!/usr/bin/env sh

set -e

mkdir -p .browser

echo
echo Preparing browser tests:

find spec -type f -name '*.spec.js' | \
***REMOVED***args -I {} sh -c \
'e***REMOVED***port f="{}"; echo $f; browserify $f -t require-globify -t brfs -***REMOVED*** ajv -u buffer -o $(echo $f | sed -e "s/spec/.browser/");'
