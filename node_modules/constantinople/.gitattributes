# Auto detect te***REMOVED***t files and perform LF normalization
* te***REMOVED***t=auto

# Custom for Visual Studio
*.cs     diff=csharp
*.sln    merge=union
*.csproj merge=union
*.vbproj merge=union
*.fsproj merge=union
*.dbproj merge=union

# Standard to msysgit
*.doc	 diff=aste***REMOVED***tplain
*.DOC	 diff=aste***REMOVED***tplain
*.doc***REMOVED*** diff=aste***REMOVED***tplain
*.DOCX diff=aste***REMOVED***tplain
*.dot  diff=aste***REMOVED***tplain
*.DOT  diff=aste***REMOVED***tplain
*.pdf  diff=aste***REMOVED***tplain
*.PDF	 diff=aste***REMOVED***tplain
*.rtf	 diff=aste***REMOVED***tplain
*.RTF	 diff=aste***REMOVED***tplain
